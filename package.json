{
  "name": "ssr",
  "version": "1.0.0",
  "description": "Super simple React SSR",
  "main": "index.js",
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.js",
    "serve-build": "node ./build/server/server.js",
    "start": "npm-run-all clean build:* serve-build",
    "clean": "rm -rf build",
    "devserver": "NODE_ENV=development webpack serve --config webpack.client.js",
    "dev": "npm-run-all clean devserver"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
