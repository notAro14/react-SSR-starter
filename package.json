{
  "name": "ssr",
  "version": "1.0.0",
  "description": "Super simple React SSR",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:client": "NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.js",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development webpack serve --config webpack.client.js",
    "serve": "node ./dist/server/server.js",
    "start": "npm-run-all clean dev"
  },
  "keywords": [],
  "author": "Aro Andriamaro",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
